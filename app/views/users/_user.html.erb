<div id="<%= dom_id user %>">
  <p class="text-blue-400 bg-pink-200">
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p class="mt-3 text-pink-800">
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

</div>
